<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <GridPane GridPane.columnIndex="1" GridPane.rowIndex="0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button mnemonicParsing="false" text="Button" />
            <Button mnemonicParsing="false" text="Button" />
            <Label GridPane.rowIndex="0" GridPane.columnIndex="0" text="Ime: " />
            <Label GridPane.rowIndex="1" GridPane.columnIndex="0" text="Prezime: " />
            <Label GridPane.rowIndex="2" GridPane.columnIndex="0" text="E-mail: " />
            <Label GridPane.rowIndex="3" GridPane.columnIndex="0" text="Korisnicko ime: " />
            <Label GridPane.rowIndex="4" GridPane.columnIndex="0" text="Lozinka: " />
            <TextField GridPane.rowIndex="0" GridPane.columnIndex="1" fx:id="unosImena" />
            <TextField GridPane.rowIndex="1" GridPane.columnIndex="1" fx:id="unosPrezimena" />
            <TextField GridPane.rowIndex="2" GridPane.columnIndex="0" fx:id="unosEmaila" />
            <TextField GridPane.rowIndex="3" GridPane.columnIndex="0" fx:id="unosKorisnickogImena" />
            <TextField GridPane.rowIndex="4" GridPane.columnIndex="0" fx:id="unosLozinke"/>
         </children>
      </GridPane>
      <ListView prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="0" GridPane.rowIndex="0" />
   </children>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
